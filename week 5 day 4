import java.io.*;
import java.util.*;

public class Shojib {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);
        int l =sc.nextInt();
        int fac = 1;
        if(l<=10 && l>0){
        for(int i = 1;i<=l;i++)
        {
            fac = fac*i;
        }
        System.out.println(fac);
    }
    }
}
