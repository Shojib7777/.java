import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ip=new Scanner(System.in);
        String s;
        char c;
        s=ip.nextLine();
        s=s.trim();
        c=ip.next().charAt(0);
        int l=s.length();
        int f=0;
        for(int i=0;i<l;i++)
        {
            char x=s.charAt(i);
            if(x==c)
            {
                f++;
            }
        }
        System.out.println(f);
    }
}
